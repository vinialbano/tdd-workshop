{
  "name": "e2e-tests",
  "version": "0.1.0",
  "description": "End-to-end tests for TDD Workshop",
  "scripts": {
    "test": "PLAYWRIGHT_HTML_OPEN=never playwright test",
    "test:ui": "playwright test --ui",
    "test:debug": "playwright test --debug",
    "test:headed": "playwright test --headed",
    "report": "PLAYWRIGHT_HTML_OPEN=never playwright test --reporter=html",
    "view-report": "playwright show-report",
    "install": "exec playwright install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "@playwright/test": "^1.42.1",
    "@types/node": "^20.17.57",
    "testcontainers": "^10.7.1",
    "typescript": "^5.8.3"
  }
}
